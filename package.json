{
  "name": "redux-essentials",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@8.1.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "format": "turbo format:check",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,js,cjs,mjs}\" --ignore-path .gitignore",
    "format:fix": "prettier --write \"**/*.{ts,tsx,md,js,cjs,mjs}\" --ignore-path .gitignore",
    "lint": "turbo lint:check lint:check-tsc",
    "lint:check": "eslint . --ignore-path .gitignore",
    "lint:check-tsc": "tsc",
    "lint:fix": "pnpm lint:check --fix --ignore-path .gitignore",
    "preview": "vite preview"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@types/eslint": "^8.37.0",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.7",
    "turbo": "^1.8.8",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-tsconfig-paths": "^4.0.7"
  }
}
